{
  "name": "waltz-ng",
  "version": "0.0.0",
  "description": "To start test server run:",
  "main": "client/main.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --recursive",
    "watch-test": "npm test -- --watch",
    "dev-server-orig": "BUILD_ENV=test webpack-dev-server --config webpack.config.js --progress --colors --port 8000 --inline --host 0.0.0.0",
    "dev-server": "webpack-dev-server --config webpack.config.js --progress --colors --port 8000 --inline --host 0.0.0.0",
    "test-server": "BUILD_ENV=test webpack-dev-server --config webpack.config.js --progress --colors --port 8000 --inline --host 0.0.0.0",
    "build-orig": "BUILD_ENV=prod webpack --config webpack.config.js -p --progress --profile --colors",
    "build": "webpack --config webpack.config.js -p --progress --profile --colors",
    "watch": "webpack --progress --watch --colors",
    "start-dev": "node lib/server-development",
    "start": "node lib/server-production"
  },
  "dependencies": {
    "angular": "^1.4.4",
    "angular-animate": "^1.4.8",
    "angular-formly": "^6.24.23",
    "angular-formly-templates-bootstrap": "^6.0.0",
    "angular-local-storage": "^0.2.2",
    "angular-sanitize": "^1.4.5",
    "angular-ui-bootstrap": "^0.14.3",
    "angular-ui-grid": "~3.0.7",
    "angular-ui-router": "^0.2.15",
    "api-check": "^7.5.0",
    "bootstrap": "^3.3.5",
    "bootstrap-sass": "~3.3.5",
    "d3": "~3.5.5",
    "font-awesome": "^4.4.0",
    "lodash": "~3.8.0",
    "moment": "^2.10.6",
    "ng-tags-input": "^3.0.0",
    "satellizer": "^0.12.5",
    "ui-select": "^0.13.1",
    "ng-redux": "~3.3.1",
    "redux-thunk": "~1.0.3",
    "numeral": "~1.5.3"
  },
  "devDependencies": {
    "babel-core": "~5.2.17",
    "babel-loader": "~5.0.0",
    "css-loader": "~0.12.0",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^1.6.1",
    "node-libs-browser": "~0.5.0",
    "node-sass": "~2.1.1",
    "raw-loader": "^0.5.1",
    "sass-loader": "~0.5.0",
    "style-loader": "~0.12.2",
    "url-loader": "~0.5.5",
    "webpack": "~1.9.4",
    "mocha": "~2.2.5"
  },
  "author": "",
  "license": "MIT"
}
